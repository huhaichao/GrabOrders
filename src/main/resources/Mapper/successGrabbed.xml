<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.lightina.GrabOrders.dao.SuccessGrabbedMapper">
    <insert id="insertInfo" parameterType="SuccessGrabbed">
        INSERT into user_order
        (user_id,order_id,create_time)
        VALUES
        (#{sg.userId},#{sg.orderId},#{sg.createTime})
    </insert>

</mapper>